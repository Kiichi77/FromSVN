package cn.com.kxcomm.ipmitool.entity;

/**
 * 
 * 服务器支持能力范围
 * 
 * @author zhangjh 新增日期：2012-8-22
 * @since ipmi_sdk
 */
public class Capability {
	/**
	 * 最大并发活动策略
	 */
	private Integer maxConcurrentActivePolicy;
	/**
	 * 最大功耗
	 */
	private Integer maxPower;
	/**
	 * 最小功耗
	 */
	private Integer minPower;
	/**
	 * 最小校正时间
	 */
	private Integer minCorrectionTime;
	/**
	 * 最大校正时间
	 */
	private Integer maxCorrectionTime;
	/**
	 * 最小统计报表期
	 */
	private Integer minStatisticsReportPeriod;
	/**
	 * 最大统计报表期
	 */
	private Integer maxStatisticsReportPeriod;
	/**
	 * 域：0 系统 1 CPU 2 内存
	 */
	private Integer domain;
	public Integer getMaxConcurrentActivePolicy() {
		return maxConcurrentActivePolicy;
	}
	public void setMaxConcurrentActivePolicy(Integer maxConcurrentActivePolicy) {
		this.maxConcurrentActivePolicy = maxConcurrentActivePolicy;
	}
	public Integer getMaxPower() {
		return maxPower;
	}
	public void setMaxPower(Integer maxPower) {
		this.maxPower = maxPower;
	}
	public Integer getMinPower() {
		return minPower;
	}
	public void setMinPower(Integer minPower) {
		this.minPower = minPower;
	}
	public Integer getMinCorrectionTime() {
		return minCorrectionTime;
	}
	public void setMinCorrectionTime(Integer minCorrectionTime) {
		this.minCorrectionTime = minCorrectionTime;
	}
	public Integer getMaxCorrectionTime() {
		return maxCorrectionTime;
	}
	public void setMaxCorrectionTime(Integer maxCorrectionTime) {
		this.maxCorrectionTime = maxCorrectionTime;
	}
	public Integer getMinStatisticsReportPeriod() {
		return minStatisticsReportPeriod;
	}
	public void setMinStatisticsReportPeriod(Integer minStatisticsReportPeriod) {
		this.minStatisticsReportPeriod = minStatisticsReportPeriod;
	}
	public Integer getMaxStatisticsReportPeriod() {
		return maxStatisticsReportPeriod;
	}
	public void setMaxStatisticsReportPeriod(Integer maxStatisticsReportPeriod) {
		this.maxStatisticsReportPeriod = maxStatisticsReportPeriod;
	}
	public Integer getDomain() {
		return domain;
	}
	public void setDomain(Integer domain) {
		this.domain = domain;
	}
	@Override
	public String toString() {
		return "Capability [maxConcurrentActivePolicy="
				+ maxConcurrentActivePolicy + ", maxPower=" + maxPower
				+ ", minPower=" + minPower + ", minCorrectionTime="
				+ minCorrectionTime + ", maxCorrectionTime="
				+ maxCorrectionTime + ", minStatisticsReportPeriod="
				+ minStatisticsReportPeriod + ", maxStatisticsReportPeriod="
				+ maxStatisticsReportPeriod + ", domain=" + domain + "]";
	}
}
