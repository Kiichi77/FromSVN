<?xml version="1.0"  encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="invoice" extends="global" namespace="/invoice">
	    <action name="invoice" class="cn.com.kxcomm.contractmanage.web.action.InvoiceAction">
	    <result name="success" type="json">
	         <param name="root">result</param>
	         <param name="noCache">true</param>  
             <param name="ignoreHierarchy">false</param>  
             <param name="contentType">text/html</param>
	      </result>
	    </action>
	    <action name="purchaseinvoice" class="cn.com.kxcomm.contractmanage.web.action.PurchaseInvoiceAction">
	    <result name="success" type="json">
	         <param name="root">result</param>
	         <param name="noCache">true</param>  
             <param name="ignoreHierarchy">false</param>  
             <param name="contentType">text/html</param>
	      </result>
	    </action>
	</package>
</struts>

