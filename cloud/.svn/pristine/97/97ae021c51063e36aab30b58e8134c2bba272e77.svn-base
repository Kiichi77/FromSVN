package cn.com.kxcomm.contractmanage.status;

/**
 * 
 * 机型配置状态定义
 * 
 * @author lujia 新增日期：2013-3-8
 * @since ContractManage
 */
public class S_ConfigModels {
	/**
	 * 无效,不可以修改配置单
	 */
	public static final int CONFIG_VAINU  = 0;
	/**
	 * 未审核或者审核不通过,报价表未审核，则可以修改配置单
	 */
	public static final int CONFIG_VALID  = 1;
	/**
	 * 已审核,报价表已审核或者已经提交审核，则不可以修改配置单
	 */
	public static final int CONFIG_CHECKED = 2;
}
